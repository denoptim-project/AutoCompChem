#!/bin/bash

#TODO see also data/knowledgebase/gaussian/e007.0-MemoryAllocationFailure.json_TODO
echo "Skipped: to upgrade!"
exit 0

if [ ! -f t40_restart.inp ] ; then
  echo NOT Passed - ERROR 1
  exit -1
fi

if ! grep -q 'Step 1 returned Error Message: e0060-MemoryAllocationFailure' t40.log; then 
  echo NOT Passed - ERROR 2
  exit -1
fi

n=$(grep -c '%MEM=15000MB' t40_restart.inp)
if [[ $n != 5 ]] ; then
  echo NOT Passed - ERROR3
  exit -1
fi

n=$(grep -c '0 1' t40_restart.inp)
if [[ $n != 5  ]] ; then
  echo NOT Passed - ERROR 4
  exit -1
fi

n=$(grep -c 'Ru   -0.0257   -0.0018   0.0046' t40_restart.inp)
if [[ $n != 1 ]] ; then
  echo NOT Passed - ERROR 5
  exit -1
fi

grep -q 'Termination status: 0' t40.log
if [ $? == 0 ] ; then 
  echo Passed
  exit 0
else
  echo NOT Passed 6
  exit -1
fi
