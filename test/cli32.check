#!/bin/bash

function not_passed() {
  echo "NOT Passed: check condition leading to line '$1' of '$0'"
  exit -1
}

if [ ! -f "cli32.log" ] ; then not_passed $LINENO ; fi

if ! grep -q -i "Iteration=11\.0 " "cli32.log" ; then not_passed $LINENO ; fi
if ! grep -q -i " varA=3\.87933" "cli32.log" ; then not_passed $LINENO ; fi
if ! grep -q -i " varA=14\.8793" "cli32.log" ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i " varB=\[4\.070.*, 4\.070.*\] " "cli32.log")
if [ 12 != "$n" ] ; then not_passed $LINENO ; exit 0 ; fi

n=0; n=$(grep -c -i "Initiating Shell Job" "cli32.log")
if [ 12 != "$n" ] ; then not_passed $LINENO ; exit 0 ; fi

n=0; n=$(grep -c -i "Termination status: 0" "cli32.log")
if [ 1 == "$n" ] ; then echo Passed ; exit 0 ; fi
not_passed $LINENO
exit -1
