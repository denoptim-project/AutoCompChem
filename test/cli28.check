#!/bin/bash

function not_passed() {
  echo "NOT Passed: check condition leading to line '$1' of '$0'"
  exit -1
}

if [ ! -f "cli28.log" ] ; then not_passed $LINENO ; fi

if ! grep -q -i "(#Conformers = 8)" "cli28.log" ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "ConformationalCoordinate" "cli28.log")
if [ 2 != "$n" ] ; then not_passed $LINENO ; fi

if [ ! -f "cli28_conformers.sdf" ] ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "\$\$\$\$" "cli28_conformers.sdf")
if [ 8 != "$n" ] ; then not_passed $LINENO ; fi



if grep -q 'Termination status: 0' "cli28.log" ; then echo Passed ; exit 0 ; fi
not_passed $LINENO
exit -1
