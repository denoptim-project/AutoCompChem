#!/bin/bash

function not_passed() {
  echo "NOT Passed: check condition leading to line '$1' of '$0'"
  exit -1
}

if [ ! -f "cli27.log" ] ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "Initiating" "cli27.log")
if [ 3 != "$n" ] ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "cli27\.xcontrol" "cli27.log")
if [ 2 != "$n" ] ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "cli27\.sdf" "cli27.log")
if [ 2 != "$n" ] ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "xtbinit\.sdf" "cli27.log")
if [ 2 != "$n" ] ; then not_passed $LINENO ; fi

if grep -q 'Termination status: 0' "cli27.log" ; then echo Passed ; exit 0 ; fi
not_passed $LINENO
exit -1
