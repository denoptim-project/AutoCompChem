#!/bin/bash

function not_passed() {
  echo "NOT Passed: check condition leading to line '$1' of '$0'"
  exit -1
}

if [ ! -f cli35.log ] ; then not_passed $LINENO ; fi

n=0; n=$(grep -c -i "DISTANCE .*atmIDs:\[0,58\]" cli35.log)
if [ 3 != "$n" ] ; then not_passed $LINENO ; exit 0 ; fi

if ! grep -q -i "DISTANCE .*atmIDs:\[0,58\].*CURRENTVALUE=3.645" cli35.log; then not_passed $LINENO ; exit 0 ; fi

if ! grep -q -i "DISTANCE .*atmIDs:\[0,58\].*CURRENTVALUE=12.695" cli35.log; then not_passed $LINENO ; exit 0 ; fi

if ! grep -q -i "DISTANCE .*atmIDs:\[0,58\].* VALUE=5.455" cli35.log; then not_passed $LINENO ; exit 0 ; fi

n=0; n=$(grep -c -i "Termination status: 0" cli35.log)
if [ 1 == "$n" ] ; then echo Passed ; exit 0 ; fi
not_passed $LINENO
exit -1
