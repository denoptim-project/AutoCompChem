#!/bin/bash

if [ ! -f "t95-mol.inp" ]
then
echo NOT Passes ERROR 1
exit -1
fi

n=0;n=$(grep -c "\-222 *111" t95-mol.inp) 
if [ $n -ne 1 ] ; then echo NOT Passes ERROR 2 ; exit -1 ; fi

n=0;n=$(grep -c "C " t95-mol.inp)
if [ $n -ne 1 ] ; then echo NOT Passes ERROR 3 ; exit -1 ; fi

n=0;n=$(grep -c "\-444 *333" t95-mol.inp)
if [ $n -ne 1 ] ; then echo NOT Passes ERROR 4 ; exit -1 ; fi

na=$(grep -ni "STO-3G" t95-mol.inp | head -n 1 | sed 's/:/ /' | awk '{print $1}')
nb=$(grep -ni "pcmdoc" t95-mol.inp | head -n 1 | sed 's/:/ /' | awk '{print $1}')
if [ "$na" -gt "$nb" ] ; then echo NOT Passed ERROR 5 ; exit -1 ; fi

grep -q 'Termination status: 0' "t95.log"
if [ "$?" == 0 ] ; then
echo Passed
exit 0
else
echo NOT Passed ERROR 6
exit -1
fi

