#!/bin/bash

grep -q "Found 'CHARGE' option" t8.log
if [ $? != 0 ] ; then
echo NOT Passed
exit -1
fi

grep -q "Found 'SPIN_MULTIPLICITY' option" t8.log
if [ $? != 0 ] ; then
echo NOT Passed
exit -1
fi

grep -q "From c = -34 and s.m. = 55 to c = -34 and s.m. = 3" t8.log
if [ $? != 0 ] ; then
echo NOT Passed
exit -1
fi

grep -q 'Termination status: 0' t8.log
if [ $? == 0 ] ; then 
echo Passed
exit 0
else
echo NOT Passed
exit -1
fi
