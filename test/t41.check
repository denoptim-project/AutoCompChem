#!/bin/bash

if [ ! -f t41.inp ]
then
echo NOT Passed 1
exit -1
fi

grep -q 'Step 1 returned Error Message: e0060-MemoryAllocationFailure' t41.log
if [ $? != 0 ] ; then  
echo NOT Passed 2
exit -1
fi

n=$(grep -c '%MEM=29000MB' t41.inp)
if [[ $n != 5 ]] ; then
echo NOT Passed 3
exit -1
fi

n=$(grep -c '0 1' t41.inp)
if [[ $n != 5  ]] ; then
echo NOT Passed 4
exit -1
fi

n=$(grep -c 'Ru   -0.0257   -0.0018   0.0046' t41.inp)
if [[ $n != 1 ]] ; then
echo NOT Passed 5
exit -1
fi

grep -q 'Termination status: 0' t41.log
if [ $? == 0 ] ; then 
  echo Passed
  exit 0
else
  echo NOT Passed 6
  exit -1
fi
