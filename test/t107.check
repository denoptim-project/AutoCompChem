#!/bin/bash
n=$(grep "Adding dummy on source atom" t107.log -c)
if [ "$n" != "15" ] ; then
  echo NOT Passed 1
  exit -1
fi

if [ ! -f t107-out.sdf ] ; then
  echo NOT Passed 2
  exit -1
fi

n=$(grep " Du " t107-out.sdf -c)
if [ "$n" != "15" ] ; then
  echo NOT Passed 3
  exit -1
fi

grep -q 'Termination status: 0' t107.log
if [ $? == 0 ] ; then 
  echo Passed
  exit 0
else
  echo NOT Passed 4
  exit -1
fi
