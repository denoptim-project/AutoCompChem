#
# Parameters for Automated Computational Chemistry
#
# Parsing data from Orca output
VERBOSITY: 1
TASK: AnalyseORCAOutput
JobOutputFile: ../t118-orca.log
ROOTPATHNAMEOUTPUT: t118_analysis
PRINTALLGEOMETRIES: XYZ t118_allGeoms.xyz
PRINTLASTGEOMETRY: SDF t118_lastGeom.sdf
PRINTVIBMODES: 6 7 t118_vibModes_6_7.xyz
TEMPLATECONNECTIVITY ../t118-tmpl.sdf
CALCULATEENERGY: QUASIHARM 100
DETECTKINDOFCRITICALPOINT: LOWESTFREQ 30
