#!/bin/bash

n=$(grep -c -i "_ONLY_FOR_TEST <-" t55.log)
if [ $n != 3 ] ; then
echo NOT Passed ERROR 1
exit -1
fi

grep -q -i "File:../t55-nwc.out Steps:3 NormalTerm:false Error:test3_ONLY_FOR_TEST" t55.log
if [ $? != 0 ] ; then
echo NOT Passed ERROR 2
exit -1
fi

grep -i -q "NWChem output file contains: 3 steps" t55.log
if [ $? != 0 ] ; then
echo NOT Passed ERROR 3
exit -1
fi

grep -q 'Termination status: 0' t55.log
if [ $? == 0 ] ; then 
echo Passed
exit 0
else
echo NOT Passed ERROR 4
exit -1
fi
