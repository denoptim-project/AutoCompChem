#!/bin/bash

if [ ! -f t2-mol.inp ] ; then
  echo NOT Passed. ERROR 1
  exit -1
fi

if ! grep -q "HF/LANL2MB" t2-mol.inp ; then
  echo NOT Passed. ERROR 2
  exit -1
fi

if ! grep -q "^-5 *3$" t2-mol.inp ; then
  echo NOT Passed. ERROR 3
  exit -1
fi

if ! grep -q "^H * 5.14.* -3.392.* -2.72" t2-mol.inp ; then
  echo NOT Passed. ERROR 4
  exit -1
fi

if ! grep -q "^H * 3.47.* 3.9.* -2.125" t2-mol.inp ; then
  echo NOT Passed. ERROR 5
  exit -1
fi

if grep -q 'Termination status: 0' t2.log ; then
  echo Passed
  exit 0
else
  echo NOT Passed. ERROR 99
  exit -1
fi
